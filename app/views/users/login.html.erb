<h1>TRELORA</h1>
<h2>Listing Consultation Tool</h2>

<%= form_tag login_path do %>
  <div class="form-group">
    <%= label_tag :member_email, "Member Email" %>
    <%= email_field_tag :member_email, nil, class: "form-control", 'aria-describedby' => "emailHelp" %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, nil, class: 'form-control' %>
  </div>
  <%= submit_tag "Login", class: "btn btn-primary" %>
<% end %>
